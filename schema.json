{
  "$schema": "http://json-schema.org/draft-04/hyper-schema#",
  "type": "object",
  "properties": {
    "readme": {
      "type": "string",
      "title": "Wan 2.1 T2V: Open and Advanced Large-Scale Video Generative Models",
      "author": "Ang Wang, Baole Ai, Bin Wen, Chaojie Mao, Chen-Wei Xie, Di Chen, Feiwu Yu, Haiming Zhao, Jianxiao Yang, Jianyuan Zeng",
      "code": "https://github.com/Wan-Video/Wan2.1",
      "paper": "https://arxiv.org/abs/2503.20314",
      "page": "https://wan.video/",
      "tags": [
        "Text to Video"
      ],
      "widget": "readme"
    },
    "positive_prompt": {
      "type": "string",
      "description": "Positive Prompt",
      "widget": "textarea"
    },
    "negative_prompt": {
      "type": "string",
      "description": "Negative Prompt",
      "widget": "hidden"
    },
    "width": {
      "type": "integer",
      "widget": "select",
      "description": "Width",
      "oneOf": [
        {
          "description": "576",
          "enum": [
            576
          ]
        },
        {
          "description": "720",
          "enum": [
            720
          ]
        },
        {
          "description": "1024",
          "enum": [
            1024
          ]
        },
        {
          "description": "1280",
          "enum": [
            1280
          ]
        }
      ],
      "default": 1024
    },
    "height": {
      "type": "integer",
      "widget": "select",
      "description": "Height",
      "oneOf": [
        {
          "description": "576",
          "enum": [
            576
          ]
        },
        {
          "description": "720",
          "enum": [
            720
          ]
        },
        {
          "description": "1024",
          "enum": [
            1024
          ]
        },
        {
          "description": "1280",
          "enum": [
            1280
          ]
        }
      ],
      "default": 576
    },
    "length": {
      "type": "integer",
      "description": "Length",
      "widget": "hidden"
    },
    "batch_size": {
      "type": "integer",
      "description": "Batch Size",
      "widget": "hidden"
    },
    "shift": {
      "type": "integer",
      "description": "Shift"
    },
    "cfg": {
      "type": "number",
      "description": "CFG"
    },
    "sampler_name": {
      "type": "string",
      "description": "Sampler Name"
    },
    "scheduler": {
      "type": "string",
      "description": "Scheduler"
    },
    "steps": {
      "type": "integer",
      "description": "Steps",
      "widget": "hidden"
    },
    "seed": {
      "type": "integer",
      "description": "Seed (if 0 random)"
    },
    "fps": {
      "type": "integer",
      "description": "FPS"
    },
    "filename_prefix": {
      "type": "string",
      "description": "Filename Prefix",
      "widget": "hidden"
    },
    "is_cinematic": {
      "type": "boolean",
      "description": "Is Cinematic"
    },
    "example": {
      "type": "string",
      "image1": "https://files.catbox.moe/tn4nad.mp4",
      "image2": "https://files.catbox.moe/37vd3c.mp4",
      "image3": "https://files.catbox.moe/uaeupg.mp4",
      "image4": "https://files.catbox.moe/wv5trg.mp4",
      "video_aspect_ratio": "60%",
      "line1": "Video: 1280x720 pixels, 5 seconds | Gen Time: ~6 minutes",
      "line2": "Video: 1024x576 pixels, 5 seconds | Gen Time: ~3 minutes",
      "widget": "example"
    }
  },
  "buttons": [
    {
      "id": "enter",
      "label": "ðŸ¥ª Enter"
    }
  ]
}